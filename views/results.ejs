<%- include('partials/header')%>

<div class="container">
    <h2>Search for a movie!</h2>
<%if(data['Search']){%>
    <% data['Search'].forEach(function(movie){ %>  
        <li>
                <strong><%= movie.Title %></strong> - <%= movie.Year %>
             </li>  
    <%});%>
<%}else{%>
        <h3>No movie was found</h3>
<%}%>
<a href="/">search again</a>
</div>

<%- include('partials/footer')%>